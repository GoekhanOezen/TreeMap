    "treemap",
    "non-recursive"
  ],
  "dependencies": {
    "d3": "^5.5.0"
  },
  "author": "Gökhan Özen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GoekhanOezen/TreeMap/issues"
  },
  "homepage": "https://github.com/GoekhanOezen/TreeMap#readme",
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "concurrently": "^3.6.1",
    "lite-server": "^2.4.0",
    "typescript": "^3.0.1",
    "node-sass": "^4.10.0"
  }
}
