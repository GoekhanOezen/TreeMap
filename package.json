{
  "name": "treemap",
  "version": "0.0.1",
  "description": "d3.js based, non-recursive data displaying treemap",
  "main": "src/treemap.ts",
  "scripts": {
    "test": "we are live!",
    "scss:w": "node-sass -w src/ -o dist/",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
